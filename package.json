    },
    "devDependencies": {
        "@types/node": "^20.14.10",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-jest": "^29.12.1",
        "eslint-plugin-tsdoc": "^0.5.0",
        "prettier": "^3.2.5",
        "typescript": "^5.9.3",
